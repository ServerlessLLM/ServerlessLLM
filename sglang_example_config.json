{
  "model": "microsoft/DialoGPT-medium",
  "backend": "sglang",
  "num_gpus": 1,
  "placement_config": {
    "placement_strategy": "storage_aware",
    "max_instances": 1,
    "min_instances": 0
  },
  "backend_config": {
    "server_url": "http://localhost:8123",
    "use_native_sglang": true,
    "timeout": 30,
    "cache_size": 500,
    "cache_ttl": 1800
  },
  "router_config": {
    "routing_strategy": "round_robin"
  }
}